== function get_room_number_by_occupant(occupant)
{
    - occupant == room_1_occupant:
        ~ return 1
    - occupant == room_2_occupant:
        ~ return 2
    - occupant == room_3_occupant:
        ~ return 3
    - occupant == room_4_occupant:
        ~ return 4
    - occupant == room_5_occupant:
        ~ return 5
    - occupant == room_6_occupant:
        ~ return 6
}